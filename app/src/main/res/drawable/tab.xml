<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="2dp" />
            <solid android:color="#00b0f0" />
            <padding
                android:bottom="0dp"
                android:left="0dp"
                android:right="0dp"
                android:top="0dp" />
            <stroke
                android:width="1dp"
                android:color="#00b0f0"
                ></stroke>
        </shape>
    </item>


</layer-list>